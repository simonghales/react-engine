  },
  "dependencies": {
    "@atlaskit/tree": "^8.2.0",
    "@stitches/core": "^0.0.2",
    "@stitches/react": "^0.0.2",
    "lodash-es": "^4.17.21",
    "react-google-font-loader": "^1.1.0",
    "react-icons": "^4.2.0",
    "reset.css": "^2.0.2",
    "zustand": "^3.3.3"
