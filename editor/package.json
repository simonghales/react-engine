{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "zustand": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "editor",
  "author": "Simon Hales",
  "module": "dist/editor.esm.js",
  "size-limit": [
    {
      "path": "dist/editor.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/editor.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "husky": "^4.3.8",
    "react": "^17.0.1",
    "react-dom": "^16.0.0 || ^17.0.1",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "zustand": "^3.3.1"
  },
  "dependencies": {
    "leva": "^0.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-onclickoutside": "^6.10.0",
    "react-outside-click-handler": "^1.3.0",
    "react-shadow": "^19.0.2",
    "styled-components": "^5.2.1",
    "styled-reset": "^4.3.4",
    "uuid": "^8.3.2"
  }
